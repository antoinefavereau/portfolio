#Begin Really Simple Security
<IfModule mod_rewrite.c>
	RewriteEngine on
	RewriteCond %{HTTP:X-Forwarded-Proto} !https
	RewriteCond %{REQUEST_URI} !wp-content\/cache\/(all|wpfc-mobile-cache)
	RewriteRule ^(.*)$ https://%{HTTP_HOST}/$1 [R=301,L]
</IfModule>

Options -Indexes
